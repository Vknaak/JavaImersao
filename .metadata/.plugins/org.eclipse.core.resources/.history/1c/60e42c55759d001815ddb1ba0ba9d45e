package br.com.tt;

public class Cliente {
	
	private String nome;
	private String cpf;
	
	String escreverNome(String parametroNome){
		
		if(parametroNome.length() < 3) {
			new Exception("Nome necessita ser completo");
			
		}		
		nome = parametroNome;
		return nome;
		
		
		
	}

	public String pegarNome() {
		return nome;
		
		
	}
	
	
}
